abstract class Animal {
    String a = " is animal";
    abstract void look();
    void show() {
        System.out.println("Zoo");
    }
}

class Chiken extends Animal {
    Chiken() {
        look();
    }
    void look() {
        System.out.println("Chicken" + a);
    }
    void display() {
        System.out.println("two wings");
    }
}

public class Test {
    public static void main(String[] args) {
        Animal a = new Chicken();
        a.show();
    }
}

/* 풀이: 
Chicken is animal 
Zoo
 * 
 * 
 */